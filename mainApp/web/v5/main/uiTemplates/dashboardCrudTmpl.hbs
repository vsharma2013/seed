<div class="modal fade" id="dashboardCrudModel" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title">Dashboard</h4>
        </div>
        <div class="modal-body" style="height:340px;">
            <form class="form-horizontal">
              <div class="col-md-12">
                <div class="panel-body">                                                                        
                    
                    <div class="form-group">
                        <label class="col-md-3 col-xs-12 control-label">Name</label>
                        <div class="col-md-8 col-xs-12">                                            
                            <div>
                                <input type="text" class="form-control dummyDashboardName" placeholder="Please add Dashboard name" value="{{model.name}}"/>
                            </div>                                            
                        </div>
                    </div>

                     <div class="form-group">
                        <label class="col-md-3 col-xs-12 control-label">Description</label>
                        <div class="col-md-8 col-xs-12">                                            
                            <div>
                                <input type="text" class="form-control dummyDashboardDescription" placeholder="Please add Dashboard Description" value="{{model.description}}"/>
                            </div>                                            
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-3 col-xs-12 control-label">Share Dashboard</label>
                        <div class="col-md-8 col-xs-12">                                                                                            
                            <input type="text" class="tagsinput dummyShareUserInput" value="{{usernames}}"/>
                            <span class="help-block">Type Username and press enter button</span>
                        </div>
                    </div>
                </div>
            </div>
            </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-default dummySaveDashboardData">Save</button>
        </div>
      </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div>